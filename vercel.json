{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/registration.pdf",
      "dest": "/registration.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "attachment; filename=\"registration.pdf\"",
        "Cache-Control": "no-cache"
      }
    }
  ]
}
